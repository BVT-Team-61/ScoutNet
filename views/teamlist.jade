extends layout

block content
  // Wrapper
  #wrapper

    // USER LIST
    h2 Team List
    #userList
        table
            thead
                th Team Name
                th Team Number
            tbody
              each team, i in teamlist
                tr
                  td
                    = team.name
                  td
                    = team.teamNo
                  td
                    a(href="/teams/#{team.id}").more More Info
  script(src='/javascripts/teamlist.js')
